		Sub ListaMeiosDisponiveis(dado As Variant)
		  pWheel.Enabled = False
		  pWheel.Visible = False
		  
		  Dim dado_tratado() As String = Variant_to_String(dado)
		  
		  Pop_PaymentMethod.DeleteAllRows
		  
		  For ik As Integer = 0 to dado_tratado.Ubound
		    Pop_PaymentMethod.AddRow(dado_tratado(ik))
		  Next
		  
		  Pop_PaymentMethod.Visible = True
		  lbl_PaymentMethod.Visible = True
		  
		  RaiseEvent Finalizou(dado)
		End Sub
	#tag EndEvent
	#tag Event
		Sub Parcelamento(dado As Variant)
		  pWheel.Enabled = False
		  pWheel.Visible = False
		  
		  Dim dado_tratado() As String = Variant_to_String(dado)
		  
		  Pop_Installments.DeleteAllRows
		  
		  dado_tratado(0) = Replace(dado_tratado(0), "|", "vez de")
		  dado_tratado(0) = Replace(dado_tratado(0), "|", "Total:")
		  dado_tratado(0) = Replace(dado_tratado(0), " | true", " Sem Juros")
		  dado_tratado(0) = Replace(dado_tratado(0), " | false", "")
		  
		  Pop_Installments.AddRow(dado_tratado(0))
		  
		  For ik As Integer = 1 to dado_tratado.Ubound
		    dado_tratado(ik) = Replace(dado_tratado(ik), "|", "vezes de")
		    dado_tratado(ik) = Replace(dado_tratado(ik), "|", "Total:")
		    dado_tratado(ik) = Replace(dado_tratado(ik), " | true", " Sem Juros")
		    dado_tratado(ik) = Replace(dado_tratado(ik), " | false", "")
		    
		    Pop_Installments.AddRow(dado_tratado(ik))
		  Next
		  
		  Pop_Installments.Visible = True
		  
		  RaiseEvent Finalizou(dado)
		End Sub
	#tag EndEvent
