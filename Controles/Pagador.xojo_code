	#tag Method, Flags = &h21
		Private Sub Recebido_Secure(sender As HTTPSecureSocket, s1 As String, i1 As Integer, ih As InternetHeaders, s2 As String)
		  #Pragma Unused sender
		  #Pragma Unused s1
		  #Pragma Unused ih
		  
		  If i1 = 500 Then
		    MsgBox("Erro no servido PagSeguro!" + EndOfLine + EndOfLine + "Tente novamente mais tarde" + EndOfLine + s2)
		    Return
		  End If
		  
		  Dim xml As New XmlDocument
		  
		  Try
		    xml.LoadXml(s2)
		    Self.Init(Array("SetSession", xml.DocumentElement.FirstChild.FirstChild.Value))
		    
		  Catch e As XmlException
		    MsgBox("XML error: " + e.Message)
		    
		  End Try
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub TimesUp()
		  Timeout_Timer.Mode = Timer.ModeOff
		  Timeout_Timer.Enabled = False
		  
		  pWheel.Enabled = False
		  pWheel.Visible = False
		  
		  img_Brand.Visible = False
		  img_Brand.Picture = Nil
		  img_Brand.URL = ""
		  fld_Brand.Text = ""
		  
		  RaiseEvent Finalizou("Tempo Esgotado")
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub Venda_Confirmada(sender As HTTPSecureSocket, s1 As String, i1 As Integer, ih As InternetHeaders, s2 As String)
		  #Pragma Unused sender
		  #Pragma Unused s1
		  #Pragma Unused i1
		  #Pragma Unused ih
		  
		  pBar.Enabled = False
		  pBar.Visible = False
		  
		  If s2 = "Unauthorized" Then
		    MsgBox(s2)
		    
		  Else
		    Dim xml As New XmlDocument
		    Try
		      xml.LoadXml(s2)
		      MsgBox(xml.ToString)
		      
		    Catch e As XmlException
		      MsgBox("XML error: " + e.Message)
		      
		    End Try
		    
		  End If
		End Sub
	#tag EndMethod


