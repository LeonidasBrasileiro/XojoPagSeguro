	#tag Method, Flags = &h0
		Sub Inicia_Venda(Valor As Currency)
		  loc_Valor = Valor
		  
		  myHash = ""
		  my_CC_Token = ""
		  
		  fld_Valor.Text = Format(loc_Valor, "#.00")
		  
		  Dim dadosform As New Dictionary
		  
		  dadosform.Value("email") = Session.Pag_Seguro_email
		  dadosform.Value("token") = Session.Pag_Seguro_token
		  
		  Dim buscador As New HTTPSecureSocket
		  
		  buscador.requestHeaders.DeleteAllHeaders
		  
		  buscador.requestHeaders.AppendHeader("Content-Type", Session.Pag_Seguro_Content_Type)
		  
		  If dadosform.Count > 0 Then
		    buscador.SetFormData(dadosform)
		  End If
		  
		  buscador.Secure = True
		  buscador.ConnectionType = HTTPSecureSocket.TLSv1
		  
		  
		  AddHandler buscador.PageReceived, WeakAddressOf Recebido_Secure
		  AddHandler buscador.Error, WeakAddressOf Erro_Recebido_Secure
		  
		  buscador.Post(Session.Pag_Seguro_Sessions_URL)
		End Sub
	#tag EndMethod

