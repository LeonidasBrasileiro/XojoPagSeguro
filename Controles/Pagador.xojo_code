	#tag Method, Flags = &h21
		Private Function Check_Fields_OK_CreditCard() As Boolean
		  Dim c() As String
		  
		  If img_Brand.Visible = False Then
		    c.Append("Bandeira válida")
		  End If
		  
		  If Pop_Installments.Text = "" Then
		    c.Append("Parcelas")
		  End If
		  
		  If fld_CardNumber.Text = "" Then
		    c.Append("Card Number")
		  End If
		  
		  if fld_CVV.Text = "" Then
		    c.Append("CVV")
		  End If
		  
		  if fld_CVV.Text = "" Then
		    c.Append("CVV")
		  End If
		  
		  if fld_Holder.Text = "" Then
		    c.Append("Holder Name")
		  End If
		  
		  if fld_Month.Text = "" Then
		    c.Append("Month")
		  End If
		  
		  if fld_Year.Text = "" Then
		    c.Append("Year")
		  End If
		  
		  If c.Ubound <= 0 Then
		    Return True
		  Else
		    Return False
		  End If
		End Function
