	#tag Method, Flags = &h21
		Private Sub Executa_Venda(metodo As String)
		  pBar.Enabled = True
		  pBar.Visible = True
		  
		  Session.saleitem.Value("email") = Session.Pag_Seguro_email
		  Session.saleitem.Value("token") = Session.Pag_Seguro_token
		  
		  Session.saleitem.Value("paymentMode") = Session.pag_Seguro_Payment_Mode
		  Session.saleitem.Value("senderHash") = myHash
		  Session.saleitem.Value("paymentMethod") = Session.valid_payment_options.Value(metodo)
		  Session.saleitem.Value("currency") = Session.Pag_Seguro_Currency
		  Session.saleitem.Value("reference") = Session.Pag_Seguro_Reference
		  Session.saleitem.Value("shippingAddressRequired") = Session.Pag_Seguro_Shipping_Adress_Required
		  
		  If Session.Pag_Seguro_Shipping_Adress_Required = "true" Then
		    'DEVISE a way to get this data!
		    'Session.saleitem.Value("shippingCost") = ""
		    'Session.saleitem.Value("shippingType") = ""
		    'Session.saleitem.Value("shippingAddressCountry") = ""
		    'Session.saleitem.Value("shippingAddressState") = ""
		    'Session.saleitem.Value("shippingAddressCity") = ""
		    'Session.saleitem.Value("shippingAddressPostalCode") = ""
		    'Session.saleitem.Value("shippingAddressDistrict") = ""
		    'Session.saleitem.Value("shippingAddressStreet") = ""
		    'Session.saleitem.Value("shippingAddressNumber") = ""
		    'Session.saleitem.Value("shippingAddressComplement") = ""
		  End If
		  
		  If my_CC_Token <> "" Then
		    Dim myinst As String
		    
		    myinst = Trim(NthField(Pop_Installments.Text, " ", 4))
		    If myinst.InStr(0, ".") = 0 Then
		      myinst =  Trim(ReplaceAll(Format(CDbl(NthField(Pop_Installments.Text, " ", 4)), "#.00"), ",", "."))
		    Else
		      If Mid(myinst, myinst.Len - 1, 1) = "." Then
		        myinst = myinst + "0"
		      End If
		    End If
		    
		    Session.saleitem.Value("creditCardToken") = my_CC_Token
		    Session.saleitem.Value("installmentQuantity") = CLong(NthField(Pop_Installments.Text, " ", 1))
		    Session.saleitem.Value("installmentValue") = myinst 'ReplaceAll(Format(CDbl(NthField(Pop_Installments.Text, " ", 4)), "#.00"), ",", ".")
		    Session.saleitem.Value("noInterestInstallmentQuantity") = Session.Pag_Seguro_Sem_Juros
		    
		  End If
		  
		  Dim pagador As New HTTPSecureSocket
		  
		  pagador.requestHeaders.DeleteAllHeaders
		  
		  pagador.requestHeaders.AppendHeader("Content-Type", Session.Pag_Seguro_Content_Type)
		  
		  If Session.saleitem.Count > 0 Then
		    pagador.SetFormData(Session.saleitem)
		  End If
		  
		  pagador.Secure = True
		  pagador.ConnectionType = HTTPSecureSocket.TLSv1
		  
		  AddHandler pagador.PageReceived, WeakAddressOf Venda_Confirmada
		  AddHandler pagador.Error, WeakAddressOf Erro_Venda_Confirmada
		  
		  pagador.Post(Session.Pag_Seguro_Transactions_URL)' + "?email=" + Session.Pag_Seguro_Registered_email + "&token=" + Session.Pag_Seguro_token)
		End Sub
	#tag EndMethod

