#tag Events PreviousMonthButton
	#tag Event
		Sub Action()
		  //User wants to back up a month
		  //If not at limits of dates(Jan,2000) then do it
		  
		  Dim m,y As Integer
		  
		  is_new = True
		  
		  m = MonthPopup.ListIndex + 1
		  y = Clong(YearPopup.Text)
		  
		  If m = 1 And y > StartYear Then
		    YearPopup.ListIndex = YearPopup.ListIndex - 1
		    MonthPopup.ListIndex = 11
		  Elseif m > 1 Then
		    MonthPopup.ListIndex = MonthPopup.ListIndex - 1
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CurrentMonthButton
	#tag Event
		Sub Action()
		  //User wants to move to current month
		  
		  is_new = True
		  
		  mSelectedDate = New date
		  mSelectedDate.Day = 1
		  mSelectedDate.TotalSeconds = mSelectedDate.TotalSeconds
		  
		  Update(true)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events NextMonthButton
	#tag Event
		Sub Action()
		  //User wants to advance a month
		  //If not at limits of dates(dec,2050) then do it
		  
		  Dim m, y As Integer
		  
		  is_new = True
		  
		  m = MonthPopup.ListIndex + 1
		  y = Clong(YearPopup.Text)
		  
		  If m = 12 And y < EndYear Then
		    YearPopup.ListIndex = YearPopup.ListIndex + 1
		    MonthPopup.ListIndex = 0
		  ElseIf m < 12 Then
		    MonthPopup.ListIndex = MonthPopup.ListIndex + 1
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btn_Cancela
	#tag Event
		Sub Action()
		  If ja_acionou Then Return
		  ja_acionou = True
		  is_cancelled = True
		  mSelectedDate = Nil
		  self.Close
		End Sub
	#tag EndEvent
