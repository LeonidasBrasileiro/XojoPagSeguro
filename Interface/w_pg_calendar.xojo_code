#tag Events CalendarDateButton
	#tag Event
		Sub Action(index as Integer)
		  #Pragma Unused Index
		  
		  If ja_acionou Then Return
		  ja_acionou = True
		  
		  mSelectedDate.Day = Clong(Me.Caption)
		  
		  Ativa_Botoes(True)
		  
		  Self.Close
		  
		End Sub
	#tag EndEvent
