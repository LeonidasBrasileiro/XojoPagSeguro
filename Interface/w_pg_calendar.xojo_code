	#tag Event
		Sub SelectionChanged()
		  //User has changed month
		  //Update mSelectedDate and update window
		  
		  if is_new then
		    
		    mSelectedDate.Month = Me.ListIndex + 1
		    
		    Update(false)
		    
		  end if
		End Sub
	#tag EndEvent
