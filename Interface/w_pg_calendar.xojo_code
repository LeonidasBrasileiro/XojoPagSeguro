		Sub MouseDown(X As Integer, Y As Integer, Details As REALbasic.MouseEvent)
		  #Pragma Unused X
		  #Pragma Unused Y
		  #Pragma Unused Details
		  
		  is_new = True
		End Sub
	#tag EndEvent
