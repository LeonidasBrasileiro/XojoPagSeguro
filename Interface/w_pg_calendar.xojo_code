#tag WindowCode
	#tag Event
		Sub KeyPressed(Details As REALbasic.KeyEvent)
		  if Details.KeyCode = Details.KeyEscape then
		    self.Close
		  end if
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub Ativa_Botoes(opt As Boolean)
		  For i As Integer = 0 to 36
		    CalendarDateButton(i).Enabled = opt
		  Next
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Init(origem As String, X As Integer, Y As Integer, optional ano_ini As Integer = -1, optional ano_fim As Integer = -1)
		  me.Top = X
		  me.Left = Y
		  
		  origem_data = origem
		  ja_acionou = False
		  mSelectedDate = New Date
		  
		  iamtoday = mSelectedDate.Day
		  iammonth = mSelectedDate.Month
		  iamyear = mSelectedDate.Year
		  
		  mSelectedDate.Day = 1
		  
		  If ano_ini  <> -1then
		    StartYear = ano_ini
		  Else
		    StartYear = kDefaultStartYear
		  End If
		  
		  If ano_fim  <> -1then
		    EndYear = ano_fim
		  Else
		    EndYear = kDefaultEndYear
		  End If
		  
		  LoadYearPopup
		  
		  LoadMonthPopup
		  
		  var_retorno = 0
		  
		  Update
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub LoadMonthPopup()
		  Meses = Array("Janeiro", "Fevereiro", "Mar√ßo", "Abril", "Maio", "Junho", "Julho", "Agosto", "Setembro", "Outubro", "Novembro", "Dezembro")
		  
		  MonthPopup.DeleteAllRows
		  For i As Integer = 0 To UBound(Meses)
		    MonthPopup.AddRow(Meses(i))
		  Next
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub LoadYearPopup()
		  YearPopup.DeleteAllRows
		  For i As Integer = StartYear To EndYear
		    YearPopup.AddRow(Str(i))
		  Next
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub Update(input_is_new As Boolean = False)
		  is_new = input_is_new
		  
		  MonthPopup.ListIndex = mSelectedDate.Month-1
		  
		  If mSelectedDate.Year >= StartYear And mSelectedDate.Year <= EndYear Then
		    YearPopup.ListIndex = mSelectedDate.Year-StartYear
		  ElseIf mSelectedDate.Year > EndYear Then
		    YearPopup.ListIndex = YearPopup.ListCount -1
		  Else
		    YearPopup.ListIndex = 0
		  End If
		  
		  //find how many days in this month
		  Dim monthDays As Integer
		  
		  Select Case mSelectedDate.Month
		  Case 1, 3, 5, 7, 8, 10, 12
		    monthDays = 31
		  Case 4, 6, 9, 11
		    monthDays = 30
		  Case Else
		    // Calculate February
		    If (mSelectedDate.Year / 4.0) = Floor(mSelectedDate.Year / 4.0) Then
		      monthDays = 29 // Leap year
		    Else
		      monthDays = 28
		    End If
		  End Select
		  
		  // Update all the captions of the buttons and hide as appropriate
		  Dim dayOfWeek As Integer
		  dayOfWeek = mSelectedDate.DayOfWeek
		  
		  Dim dayNum As Integer
		  For calendarButton As Integer = 0 To 36
		    DayNum = calendarButton + 2 - dayOfWeek
		    If dayNum > 0 And dayNum <= monthDays Then
		      CalendarDateButton(calendarButton).Caption = Str(dayNum)
		      CalendarDateButton(calendarButton).Visible = True
		    Else
		      CalendarDateButton(calendarButton).Visible = False
		    End If
		  Next
		  
		  for calendarButton as integer = 0 to 36
		    if CalendarDateButton(calendarButton).Caption = str(iamtoday) and MonthPopup.Text = Meses(iammonth - 1) and YearPopup.Text = str(iamyear) then
		      CalendarDateButton(calendarButton).Style = label_red
		      'exit For
		    else
		      CalendarDateButton(calendarButton).Style = label_bold
		    end if
		  next
		  
		End Sub
	#tag EndMethod


