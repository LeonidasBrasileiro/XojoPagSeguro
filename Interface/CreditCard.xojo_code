#tag WindowCode
	#tag Method, Flags = &h0
		Sub Init(Nome As String)
		  If pop_State.ListCount = 0 Then
		    pop_Country.AddRow("Brasil")
		    pop_State.AddRow("AC")
		    pop_State.AddRow("AL")
		    pop_State.AddRow("AM")
		    pop_State.AddRow("AP")
		    pop_State.AddRow("BA")
		    pop_State.AddRow("CE")
		    pop_State.AddRow("DF")
		    pop_State.AddRow("ES")
		    pop_State.AddRow("GO")
		    pop_State.AddRow("MA")
		    pop_State.AddRow("MG")
		    pop_State.AddRow("MS")
		    pop_State.AddRow("MT")
		    pop_State.AddRow("PA")
		    pop_State.AddRow("PB")
		    pop_State.AddRow("PE")
		    pop_State.AddRow("PI")
		    pop_State.AddRow("PR")
		    pop_State.AddRow("RJ")
		    pop_State.AddRow("RN")
		    pop_State.AddRow("RO")
		    pop_State.AddRow("RR")
		    pop_State.AddRow("RS")
		    pop_State.AddRow("SC")
		    pop_State.AddRow("SE")
		    pop_State.AddRow("SP")
		    pop_State.AddRow("TO")
		    
		    
		  End If
		  
		  Limpa_Controles
		  
		  fld_Name.Text = Nome
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub Limpa_Controles()
		  fld_AreaCode.Text = ""
		  fld_BirthDate.Text = ""
		  fld_Cidade.Text = ""
		  fld_CPF.Text = ""
		  fld_District.Text = ""
		  fld_Name.Text = ""
		  fld_Number.Text = ""
		  fld_Phone.Text = ""
		  fld_PostalCode.Text = ""
		  fld_Street.Text = ""
		  pop_Country.ListIndex = 0'-1
		  pop_State.ListIndex = -1
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function Load_Dictionary() As Boolean
		  If pop_State.ListIndex <> - 1 Then
		    Session.saleitem.Value("creditCardHolderName") = fld_Name.Text
		    Session.saleitem.Value("creditCardHolderCPF") = fld_CPF.Text
		    Session.saleitem.Value("creditCardHolderBirthDate") = fld_BirthDate.Text
		    Session.saleitem.Value("creditCardHolderAreaCode") = fld_AreaCode.Text
		    Session.saleitem.Value("creditCardHolderPhone") = fld_Phone.Text
		    Session.saleitem.Value("billingAddressStreet") = fld_Street.Text
		    Session.saleitem.Value("billingAddressNumber") = fld_Number.Text
		    Session.saleitem.Value("billingAddressDistrict") = fld_District.Text
		    Session.saleitem.Value("billingAddressPostalCode") = fld_PostalCode.Text
		    Session.saleitem.Value("billingAddressCity") = fld_Cidade.Text
		    Session.saleitem.Value("billingAddressState") = pop_State.Text
		    Session.saleitem.Value("billingAddressCountry") = pop_Country.Text
		    Return True
		  Else
		    pop_State.SetFocus
		    MsgBox("State!")
		    Return False
		  End If
		End Function
	#tag EndMethod


