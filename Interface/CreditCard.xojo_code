#tag Events btn_Date
	#tag Event
		Sub Action()
		  my_calendar.Show
		  my_calendar.Init("BirthDate", self.Top + me.Top - me.Height/2, self.Left + me.Left + me.Width)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btn_Accept
	#tag Event
		Sub Action()
		  If Load_Dictionary Then
		    Loaded = True
		    Self.Close
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btn_Cancel
	#tag Event
		Sub Action()
		  Loaded = False
		  Self.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events my_calendar
	#tag Event
		Sub Dismissed()
		  If me.mSelectedDate <> Nil Then
		    fld_BirthDate.Text = Data_Formatada_String(me.mSelectedDate)
		  Else
		    fld_BirthDate.Text = ""
		  End If
		  
		  fld_AreaCode.SetFocus
		End Sub
	#tag EndEvent
#tag EndEvents
